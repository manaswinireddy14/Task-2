CREATE TABLE students (
    student_id INT PRIMARY KEY,
    full_name VARCHAR(100) NOT NULL,
    email VARCHAR(150) NOT NULL UNIQUE,
    date_of_birth DATE NOT NULL,
    gender CHAR(1),
    enrollment_date DATE NOT NULL
);


INSERT INTO students VALUES
